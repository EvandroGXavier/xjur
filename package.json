{
  "name": "dr-x-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev -w apps/api\" \"npm run dev -w apps/web\"",
    "build": "turbo run build",
    "db:generate": "npx prisma@5.20.0 generate --schema=packages/database/prisma/schema.prisma",
    "test": "turbo run test",
    "lint": "turbo run lint"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/passport-jwt": "^4.0.1",
    "concurrently": "^8.2.0",
    "prisma": "5.20.0",
    "turbo": "^1.10.0"
  },
  "dependencies": {
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "bcrypt": "^6.0.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1"
  }
}
