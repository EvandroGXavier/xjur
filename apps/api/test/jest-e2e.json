
{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@drx/database$": "<rootDir>/../../../packages/database/src",
    "^axios$": "axios/dist/node/axios.cjs",
    "^@whiskeysockets/baileys$": "<rootDir>/__mocks__/baileys.js"
  }
}
